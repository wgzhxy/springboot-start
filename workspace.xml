        <mapping src="bizService-0.0.1-SNAPSHOT.jar" dest="bizService/target/classes"/>
    </deploy>
    <plugins>
        <plugin name="spring_plugin"></plugin>
        <plugin name="ibatis_plugin"></plugin>
